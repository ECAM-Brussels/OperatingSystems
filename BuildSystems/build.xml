<project name="computer">
    <taskdef resource="cpptasks.tasks" />
    <target name="computer">
        <mkdir dir="obj/" />
        <cc outtype="executable" subsystem="console" outfile="computer" objdir="obj">
            <fileset dir="src" includes="*.c" />
        </cc>
    </target>
    <target name="clean">
        <delete dir="obj" />
        <delete file="computer" /> 
    </target>
</project>
